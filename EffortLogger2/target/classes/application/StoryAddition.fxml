	xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="application.StoryAdditionController">
	<children>
		<Label layoutX="236.0" layoutY="14.0" text="Story Addition">
			<font>
				<Font size="20.0" />
			</font>
		</Label>
		<Button fx:id="addStory" layoutX="71.0" layoutY="231.0"
			mnemonicParsing="false" onAction="#storeStory" text="Add Story" />
		<Button fx:id="homeButton" layoutX="249.0" layoutY="360.0"
			mnemonicParsing="false" onAction="#returnHome" text="Return to Home" />
		<TextField fx:id="description" layoutX="71.0" layoutY="151.0"
			prefHeight="26.0" prefWidth="377.0" promptText="Description..." />
		<Label layoutX="44.0" layoutY="52.0"
			text="1. Enter the specifications for this story" />
		<Label layoutX="71.0" layoutY="84.0" text="Story Title" />
		<Label layoutX="71.0" layoutY="133.0" text="Story Details" />
		<Label layoutX="44.0" layoutY="200.0"
			text="2. Press this button to generate a story with the above attributes" />
		<TextField fx:id="title" layoutX="71.0" layoutY="102.0"
			prefHeight="26.0" prefWidth="377.0" promptText="Title" />
	</children>
</AnchorPane>
